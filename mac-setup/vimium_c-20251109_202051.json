	"environment": {
		"extension": "2.12.2",
		"platform": "mac",
		"chromium": 141
	},
	"allBrowserUrls": true,
	"keyLayout": 2,
	"keyupTime": 60,
	"scrollStepSize": 50,
	"userDefinedCss": [
		"/* ====== PAGE BACKDROP (for demo only) ====== */",
		"html, body {",
		"  height: 100%;",
		"  margin: 0;",
		"  background: linear-gradient(135deg,#0b0d1e 0%, #0f1230 50%, #0a0b15 100%);",
		"  color: #f5f7ff;",
		"  font-family: \"Inter\",\"JetBrains Mono\",system-ui,-apple-system,Segoe UI,Roboto,sans-serif;",
		"}",
		".container { padding: 24px; max-width: 1200px; margin: 0 auto; }",
		"h1 { font-weight: 800; letter-spacing: 0.2px; margin: 0 0 12px; }",
		"h2 { margin: 28px 0 10px; font-weight: 700; }",
		"p.note { color: #c7cfe6; }",
		"",
		"/* ====== ANIMATIONS (subtle) ====== */",
		"@keyframes fadeIn { from { opacity: 0 } to { opacity: 1 } }",
		"@keyframes pulseGlow { 0%,100%{ box-shadow: 0 0 0 rgba(0,0,0,0) } 50%{ box-shadow: 0 0 20px rgba(181,107,255,.45), 0 0 28px rgba(74,163,255,.35) } }",
		"",
		"/* ====== SHARED COLORS ====== */",
		":root{",
		"  --bg:#0a0b15;",
		"  --panel1:#151836;",
		"  --panel2:#0c0f26;",
		"  --violet:#b56bff;",
		"  --blue:#4aa3ff;",
		"  --rose:#ff4db8;",
		"  --amber:#ffd366;",
		"  --white:#f5f7ff;",
		"  --sub:#c7cfe6;",
		"  --key-bg:#0f1222;",
		"  --hint-bg:#1a1d2e;",
		"  --border:#2a2f48;",
		"}",
		"",
		"/* ====== LINK STYLE (must look clickable) ====== */",
		"a, .VimL {",
		"  color: var(--amber);",
		"  text-decoration: underline;",
		"  text-decoration-thickness: 1px;",
		"  text-underline-offset: 2px;",
		"}",
		"a:hover, .VimL:hover { color: var(--rose); }",
		"a:visited, .VimL:visited { color: #ffe39b; }",
		"",
		"/* =========================================================",
		"   VIMIUM‑C UI THEME (SOLID DARK, NO TRANSPARENCY)",
		"   ========================================================= */",
		"",
		"/* Base overlay text */",
		".R { ",
		"  color: var(--white) !important; ",
		"  text-shadow: none !important; ",
		"  font-family: \"Inter\",\"JetBrains Mono\",system-ui,-apple-system,Segoe UI,Roboto,sans-serif !important; ",
		"}",
		"",
		"/* HELP DIALOG */",
		"#HDlg {",
		"  background: linear-gradient(135deg, #151836 0%, #0c0f26 100%) !important;",
		"  color: #f5f7ff !important;",
		"  border: 1px solid #2a2f48 !important;",
		"  box-shadow: 0 10px 40px #05060d !important;",
		"  animation: fadeIn .2s ease-out;",
		"  opacity: 1 !important;",
		"}",
		"#HelpBanner { border-bottom: 1px solid #8d61d9 !important; background: transparent !important; }",
		"#HelpTitle { color: #f5f7ff !important; }",
		".HelpSectionTitle {",
		"  color: #b56bff !important;",
		"  border-bottom: 1px solid #a77bf7 !important;",
		"  background: transparent !important;",
		"}",
		".title2, .HelpCommandName { color: #ffd366 !important; }",
		".HelpCommandInfo { color: #c7cfe6 !important; }",
		"",
		"/* Help Keys (high contrast) */",
		".HelpKey {",
		"  background: #0f1222 !important;",
		"  color: #f5f7ff !important;",
		"  border: 1px solid #2a2f48 !important;",
		"  box-shadow: none !important;",
		"  text-shadow: none !important;",
		"  opacity: 1 !important;",
		"}",
		"",
		"/* HINTS — solid dark, readable */",
		".LH {",
		"  background: #1a1d2e !important;",
		"  color: #f5f7ff !important;",
		"  border: 2px solid #2a2f48 !important;",
		"  box-shadow: none !important;",
		"  opacity: 1 !important;",
		"  -webkit-text-fill-color: #f5f7ff !important;",
		"  transition: transform .12s ease, border-color .12s ease;",
		"  animation: fadeIn .2s ease-out;",
		"}",
		"",
		"/* Hint type borders */",
		".LH[data-type=\"link\"] { border-color: #4aa3ff !important; }",
		".LH[data-type=\"input\"], .LH[data-type=\"editable\"], .LH[data-type=\"textbox\"] { border-color: #b56bff !important; }",
		".LH[data-type=\"button\"], .LH[data-type=\"control\"], .LH[data-type=\"aria-button\"] { border-color: #ff4db8 !important; }",
		".LH[data-type=\"frame\"], .BH { border-color: #4aa3ff !important; color: #f5f7ff !important; background: #1a1d2e !important; }",
		"",
		"/* Selected / hover state */",
		".LH:hover, .LH.active, .LH[aria-selected=\"true\"], .LH[data-selected=\"true\"] {",
		"  transform: scale(1.06);",
		"  border-color: #b56bff !important;",
		"  animation: pulseGlow .5s ease-out;",
		"}",
		"",
		"/* HUD / TEE (find input in-page) */",
		".HUD, .TEE, .r {",
		"  background: linear-gradient(135deg, #151836 0%, #0c0f26 100%) !important;",
		"  color: #f5f7ff !important;",
		"  border: 1px solid #2a2f48 !important;",
		"  box-shadow: none !important;",
		"  opacity: 1 !important;",
		"  animation: fadeIn .2s ease-out;",
		"}",
		"#s { color: #b56bff !important; }",
		"#c { color: #ffd366 !important; }",
		"",
		"/* Selection color inside overlays */",
		"::selection { background: #7e57c2 !important; color: #ffffff !important; }",
		"",
		"/* ====== DEMO LAYOUT HELPERS ====== */",
		".demo-wrap { border: 1px dashed #2a2f48; padding: 12px; margin: 16px 0; }",
		".hints-demo { position: relative; min-height: 220px; background: #0f1124; border: 1px solid #2a2f48; overflow: hidden; }",
		".hints-demo .LH { position: absolute; padding: 4px 6px; border-radius: 4px; font-weight: 800; letter-spacing: 1.4px; }",
		""
	],
	"vimSync": true
}
